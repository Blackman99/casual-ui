{
  "name": "casual-ui-svelte",
  "version": "0.0.1",
  "description": "Casual UI for Svelte",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./src/index.ts",
  "scripts": {
    "build": "vite build"
  },
  "keywords": [
    "svelte",
    "svelte3",
    "ui components",
    "svelte components"
  ],
  "author": "Dongsheng Zhao",
  "license": "MIT",
  "publishConfig": {
    "main": "./dist/casual-ui-svelte.umd.js",
