    "directory": "/packages/svelte/ui"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@tsconfig/svelte": "^3.0.0",
    "casual-i18n": "workspace:*",
    "casual-styles": "workspace:*",
    "casual-types": "workspace:*",
    "casual-ui-svelte": "workspace:*",
    "casual-utils": "workspace:*",
    "clsx": "^1.1.1",
    "dayjs": "^1.11.0",
    "rollup": "^2.76.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-scss": "3",
    "sass": "^1.49.9",
    "svelte": "^3.44.0",
    "typescript": "^4.7.4"
