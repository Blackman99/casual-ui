{
  "name": "casual-ui-svelte",
  "version": "0.0.4",
  "description": "Casual UI for Svelte",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./src/index.ts",
  "scripts": {
    "build": "rollup --config rollup.config.ts --configPlugin typescript"
  },
  "keywords": [
    "svelte",
    "svelte3",
    "ui components",
    "svelte components"
  ],
  "author": "Dongsheng Zhao",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/Blackman99/casual-ui.git",
    "directory": "/packages/svelte/ui"
  },
  "publishConfig": {
    "main": "./dist/casual-ui-svelte.cjs",
    "module": "./dist/casual-ui-svelte.mjs",
    "exports": {
      ".": {
        "import": "./dist/casual-ui-svelte.mjs",
        "require": "./dist/casual-ui-svelte.cjs"
      },
      "./dist/style.css": {
        "import": "./dist/style.css",
        "require": "./dist/style.css"
      }
    }
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@tsconfig/svelte": "^3.0.0",
    "casual-i18n": "workspace:*",
    "casual-styles": "workspace:*",
    "casual-types": "workspace:*",
    "casual-ui-svelte": "workspace:*",
    "casual-utils": "workspace:*",
    "clsx": "^1.1.1",
    "dayjs": "^1.11.0",
    "rollup": "^2.76.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-scss": "3",
    "rollup-plugin-svelte": "^7.1.0",
    "sass": "^1.49.9",
    "svelte": "^3.44.0",
    "svelte-preprocess": "^4.10.6",
    "typescript": "^4.7.4"
  }
}
