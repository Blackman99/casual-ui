{
  "name": "casual-vue-repl",
  "version": "1.2.3",
  "description": "Vue component for editing Vue components",
  "main": "src/index.ts",
  "publishConfig": {
    "module": "dist/vue-repl.js",
    "main": "dist/ssr-stub.js",
    "types": "dist/repl.d.ts",
    "exports": {
      ".": {
        "import": "./dist/vue-repl.js",
        "require": "./dist/ssr-stub.js",
        "types": "./dist/repl.d.ts"
      },
      "./style.css": "./dist/style.css",
      "./dist/style.css": "./dist/style.css"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build-types": "vue-tsc -p tsconfig.build.json && api-extractor run -c api-extractor.json && rimraf dist/src",
    "prepublishOnly": "npm run build && npm run build-types"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Blackman99/casual-ui.git"
  },
  "author": "Dongsheng Zhao",
  "license": "MIT",
  "devDependencies": {
    "@babel/types": "^7.15.6",
    "@microsoft/api-extractor": "^7.19.2",
    "@types/codemirror": "^5.60.2",
    "@types/node": "^16.11.12",
    "@vitejs/plugin-vue": "^1.9.0",
    "codemirror": "^5.62.3",
    "hash-sum": "^2.0.0",
    "rimraf": "^3.0.2",
    "sucrase": "^3.20.1",
    "typescript": "^4.5.4",
    "vite": "^2.9.8",
    "vue": "^3.2.36",
    "vue-tsc": "^0.34.15"
  },
  "peerDependencies": {
    "vue": "^3.2.13"
  }
}
